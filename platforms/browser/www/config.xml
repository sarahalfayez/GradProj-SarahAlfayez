<?xml version='1.0' encoding='utf-8'?>
<widget id="speicalmoments" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <name>Special Moments</name>
    <description>
        An application for daycare centers to let parents have a real-time update about their children day status
    </description>
    <author email="sarahalfayez@csu.fullerton.edu">
        Sarah Alfayez
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
